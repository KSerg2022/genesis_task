{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["dom", "es6", "esnext"],
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "rootDir": "src",
    "noImplicitAny": false
  },
  "include": ["src/**/*.ts", "src/models/.model copy.ts"],
  "exclude": ["node_modules"]
}
